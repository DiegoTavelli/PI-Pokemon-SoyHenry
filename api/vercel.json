{
  "version": 2,
  "name": "pokemon-app-db",
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "DB_HOST":"localhost",
    "DB_PASSWORD": "postgres",
   "DB_USER": "postgres"
  }
}
