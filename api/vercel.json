{
  "version": 2,
  "name": "pokemon-app-db",
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "DB_HOST":"host-db",
    "DB_PASSWORD": "password-db",
   "DB_USER": "user-db"
  }
}
